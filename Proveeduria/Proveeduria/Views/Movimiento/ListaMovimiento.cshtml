
@{
                /**/

                ViewBag.Title = "ListaMovimiento";
                Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Movimientos
    </h1>
</section>

<!-- Main content -->
<section class="content">

    <p>
    </p>

    <!-- begin panel -->
    <div class="box">
        <div class="box-body no-padding">
            <div class="btn-group">
                <button id="butNuevo" class="btn btn-primary btn-sm btn-flat">
                    <i class="fa fa-plus"></i>  Nuevo
                </button>
            </div>
            <table id="tabMovimiento" class="table table-striped hover compact">
                <thead>
                    <tr>
                        <th>
                            Año
                        </th>
                        <th>
                            Número
                        </th>
                        <th>
                            Movimiento
                        </th>
                        <th>
                            Observacion
                        </th>
                        <th>
                            Estado
                        </th>
                        <th>
                            UsuaSolic.
                        </th>
                        <th>
                            Solicitado
                        </th>
                        <th>
                            Aprobado
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>

        </div>
    </div>
    <!-- end panel -->



</section>

@section scripts {

    <script type="text/javascript" src=@Url.Content("/Scripts/App/ListaMovimiento.js")></script>
    <script type="text/javascript">


        $(document).ready(function () {
            ListaMovimiento.init();
        });


    </script>
}



