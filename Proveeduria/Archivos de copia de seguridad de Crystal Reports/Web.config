      <!-- Load the ASP.NET Core plugin -->
      <add assembly="NLog.Web.AspNetCore" />
    </extensions>
    <!--Define Various Log Targets like files, database or asp.net trace files-->
    <targets>
      <target name="console" xsi:type="ColoredConsole" layout="${message}" />
      <!--Write logs to File  where we want to write error logs-->
      <!--Layout Renderers list https://github.com/NLog/NLog/wiki/Exception-Layout-Renderer-->
      <!-- Inner Exception		:	${exception:format=toString}${newline} -->
      <target name="default" xsi:type="File" fileName="logs/app-log.txt" archiveFileName="logs/archives/app-log.{#}.txt" archiveEvery="Day" archiveNumbering="Rolling" maxArchiveFiles="7" layout="     ---------------------------- ${level}(${longdate}) --------------------------    &#xD;&#xA;              Machine Name  : ${machinename}${newline}   &#xD;&#xA;              Exception Type  : ${exception:format=Type}${newline}      &#xD;&#xA;              Exception Message : ${exception:format=Message}${newline}     &#xD;&#xA;              Inner Exception  : ${exception:format=toString,Data:maxInnerExceptionLevel=10}${newline} &#xD;&#xA;              Stack Trace   : ${exception:format=Stack Trace}${newline}        &#xD;&#xA;              Additional Info  : ${message}  &#xD;&#xA;              -------------------------------------------------------------------&#xD;&#xA;              ----------     ${newline}     " />
    </targets>
    <rules>
      <logger name="*" minlevel="trace" writeTo="default" />
    </rules>
  </nlog>
